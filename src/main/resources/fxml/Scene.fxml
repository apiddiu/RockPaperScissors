<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aldo.rockpaperscissors.FXMLController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Button fx:id="newGame" mnemonicParsing="false" onAction="#startNewGame" text="New Game" />
            <Button fx:id="newAiGame" layoutX="105.0" mnemonicParsing="false" onAction="#startNewAiGame" text="New AI Game" />
            <TitledPane layoutX="14.0" layoutY="42.0" prefHeight="129.0" prefWidth="772.0" text="Player Options">
               <rotationAxis>
                  <Point3D />
               </rotationAxis>
               <content>
                  <GridPane prefHeight="146.0" prefWidth="770.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Button mnemonicParsing="false" onAction="#playRock" text="ROCK" />
                        <Button mnemonicParsing="false" onAction="#playPaper" text="PAPER" GridPane.columnIndex="1" />
                        <Button mnemonicParsing="false" onAction="#playScissors" text="SCISSORS" GridPane.columnIndex="2" />
                     </children>
                  </GridPane>
               </content>
            </TitledPane>
            <Label id="lblResult" fx:id="lblResult" layoutX="31.0" layoutY="265.0" text="Result:" visible="false" />
            <ProgressBar fx:id="progress" layoutX="300.0" layoutY="190.0" prefWidth="200.0" progress="0.0" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Pane>
   </children>
</AnchorPane>
